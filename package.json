{
  "name": "bank-app-js",
  "version": "1.0.0",
  "description": "A comprehensive interbank transaction system with secure JWT-based communication between banks",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate-keys": "mkdir -p keys && cd keys && openssl genpkey -algorithm RSA -out private.pem -pkcs8 -aes256 -pass pass:bankkey123 && openssl rsa -in private.pem -pubout -out public.pem -passin pass:bankkey123",
    "setup": "npm install && npm run generate-keys"
  },
  "keywords": [
    "banking",
    "interbank",
    "transactions",
    "jwt",
    "express",
    "mongodb",
    "financial",
    "api",
    "b2b",
    "payments"
  ],
  "author": "Bank Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lembitlindau/bank-app-js.git"
  },
  "bugs": {
    "url": "https://github.com/lembitlindau/bank-app-js/issues"
  },
  "homepage": "https://github.com/lembitlindau/bank-app-js#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.9",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
